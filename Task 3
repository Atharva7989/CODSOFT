#Password Generator
import random

print("CODSOFT Password Generator")
print()

small= "abcdefghijklmnopqrstuvwxyz "

caps ="ABCDEFGHIJKLMNOPQRSTUVWXYZ"

special_char="!@#$%^&*()"

print("Enter the length for Password")
print()
n=int(input())
print()

password=""

for i in range(n):
    
    password=password+random.choice(small+caps+special_char)
    
print("Password : ",password)
